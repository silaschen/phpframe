.guide{width: 100%;margin-bottom:10rpx;}
.guide image{width: 100%}
.form-group{margin: 0px;padding:3px 0px;}
#form{
  margin: 0;
  border: none;
  padding:0 20rpx 80rpx 20rpx;
}
#form button{margin-top: 10px;}
.address{font-size: 25rpx;color:#999;margin: 0px 15px;}
.address text{width: 100%;display: block;line-height: 38rpx;}
page{
  background: #f5f5f5;
  padding-bottom:60rpx;
}
/*上传图片  */ 
/*上传图片  */ 
.addimg{
  border:1rpx solid #eee;
  border-radius: 8rpx;
  padding:10rpx 20rpx;
}
.add .title .pull-right{
  font-size:28rpx;
}
.addimg .com{
  display: inline-block;
}
.addimg .com view{
  display: inline-block;
  width:150rpx;
  height:150rpx;
  margin-right:20rpx;
  position: relative;
}
/*上传后片图尺寸  */
.addimg .com image{
  width:150rpx;
  height:150rpx;
  display: block;
  margin:0 auto;
}
/*删除图片  */
.addimg .com .sfzpic text{
  position: absolute;
  padding:8rpx 12rpx;
  background: red;
  color:#fff;
  font-size:28rpx;
  top:0;
  left:0;
  line-height: 1;
}
/*添加图片图标  */

.addimg .addicon image{
  width:150rpx;
  height:150rpx;
}

/*支付底部导航  */
.pay{
  display: flex;
  display:-webkit-flex;
  position: fixed;
  bottom:0;
  width:100%;
  left:0;
}
.pay .money{
  width:30%;
  background: #fff;
  text-align: center;
  line-height: 100rpx;
  color:#129FE7;
  font-size:40rpx;
}
.pay .money .cont{
  margin-top:20rpx;
}
.pay .money text{
  font-weight: 800;
  line-height: 1;
  display: block;
}
.pay .money text:last-child{
  color:#999;
  font-size:24rpx;
  font-weight: normal;
}
#form .pay button{
  width:70%;
  margin:0;
  border:none;
}